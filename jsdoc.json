{
  "tags": {
    "allowUnknownTags": ["category"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "./node_modules/better-docs/category"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "docs-gen",
    "readme": "readme.md",
    "recurse": true,
    "verbose": true,
    "tutorials": "",
    "template": "./node_modules/better-docs"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
      "staticFiles": {
        "include": [""]
      }
    },
    "better-docs": {
      "name": "GCanvas",
      "logo": "images/logo.png",
      "title": "GCanvas",
      "css": "style.css",
      "trackingCode": "",
      "hideGenerator": false,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/guinetik/gcanvas"
        },
        {
          "label": "Tech Demo",
          "href": "https://gcanvas.guinetik.com"
        }
      ]
    }
  }
}
